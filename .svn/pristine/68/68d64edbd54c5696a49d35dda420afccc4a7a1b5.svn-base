package iMobile.entity;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@SuppressWarnings("serial")
@Entity
@Table(name = "AppModule")
public class AppModule implements Serializable
{	
	@Id
	@Column(name = "id")
	private Long id;
	
	@Column(name = "module_code")
	private String moduleCode;
	
	@Column(name = "employee_code")
	private String employeeCode;
	
	@Column(name = "name")
	private String name;
	
	@Column(name = "app_url")
	private String appUrl;
	
	@Column(name = "description")
	private String description;
	
	@Column(name = "app_icon")
	private  String appIcon;
	
	@Column(name = "create_time")
	private Date createTime;
	
	@Column(name = "app_type_id")
	private Integer appTypeId;
	
	@Column(name = "app_state")
	private Integer appState;
	
	@Column(name = "is_open")
	private Integer isOpen;
	
	@Column(name = "wel_msg")
	private String welMsg;

	public Long getId()
	{
		return id;
	}

	public void setId(Long id)
	{
		this.id = id;
	}

	public String getModuleCode()
	{
		return moduleCode;
	}

	public void setModuleCode(String moduleCode)
	{
		this.moduleCode = moduleCode;
	}

	public String getEmployeeCode()
	{
		return employeeCode;
	}

	public void setEmployeeCode(String employeeCode)
	{
		this.employeeCode = employeeCode;
	}

	public String getName()
	{
		return name;
	}

	public void setName(String name)
	{
		this.name = name;
	}

	public String getAppUrl()
	{
		return appUrl;
	}

	public void setAppUrl(String appUrl)
	{
		this.appUrl = appUrl;
	}

	public String getDescription()
	{
		return description;
	}

	public void setDescription(String description)
	{
		this.description = description;
	}

	public String getAppIcon()
	{
		return appIcon;
	}

	public void setAppIcon(String appIcon)
	{
		this.appIcon = appIcon;
	}

	public Date getCreateTime()
	{
		return createTime;
	}

	public void setCreateTime(Date createTime)
	{
		this.createTime = createTime;
	}

	public Integer getAppTypeId()
	{
		return appTypeId;
	}

	public void setAppTypeId(Integer appTypeId)
	{
		this.appTypeId = appTypeId;
	}

	public Integer getAppState()
	{
		return appState;
	}

	public void setAppState(Integer appState)
	{
		this.appState = appState;
	}

	public Integer getIsOpen()
	{
		return isOpen;
	}

	public void setIsOpen(Integer isOpen)
	{
		this.isOpen = isOpen;
	}

	public String getWelMsg()
	{
		return welMsg;
	}

	public void setWelMsg(String welMsg)
	{
		this.welMsg = welMsg;
	}
	
}
